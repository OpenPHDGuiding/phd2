msgid "General Information"
msgstr "Obecné Informace"

#: Refine_DefMap.cpp:100
msgid "Time:"
msgstr "Čas:"

#: Refine_DefMap.cpp:102
msgid "Camera:"
msgstr "Kamera:"

#: Refine_DefMap.cpp:106 Refine_DefMap.cpp:111
msgid "Master Dark Exposure Time:"
msgstr "Čas Expozice Master Dark:"

#: Refine_DefMap.cpp:108 Refine_DefMap.cpp:112
msgid "Master Dark Exposure Count:"
msgstr "Počet Expozic Master Dark:"

#: Refine_DefMap.cpp:115
