msgid "Guide scope focal length (mm)"
msgstr "Distância focal do telescópio guia (mm)"

#: profile_wizard.cpp:292
msgid "Focal length less than recommended minimum (100mm)"
msgstr ""

#: profile_wizard.cpp:308 profile_wizard.cpp:1274
#, c-format
msgid "Pixel scale: %8.2f\"/px"
msgstr "Escala de píxel: %8.2f\"/px"

#: profile_wizard.cpp:309
